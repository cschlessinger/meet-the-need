<div>
  <h1>Customer dashboard</h1>
  <%= link_to "Create a project", new_project_path %>
  <div id="tabs" class="<%= current_customer.id %>">
    <ul>
      <li><a href="#unassigned">Unassigned</a></li>
      <li><a href="#current">Current Projects</a></li>
      <li><a href="#completed">Completed</a></li>
      <li><a href="#requests">Requests</a></li>
    </ul>
    <div id="unassigned">
     <%= render "unassigned" %>
    </div>
    <div id="current">
      <%= render 'current' %>
    </div>
    <div id="completed">
      <%= render "completed" %>
    </div>
    <div id="requests">
     <%= render "requests" %>
    </div>
  </div>
</div>
