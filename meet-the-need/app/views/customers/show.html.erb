<div>
  <h1>Customer dashboard</h1>
  <%= link_to "Create a project", new_project_path %>
  <div id="tabs" class="<%= current_customer.id %>">
    <ul>
      <li><a href="#tabs-1">Unassigned</a></li>
      <li><a href="#tabs-2">Current Projects</a></li>
      <li><a href="#tabs-3">Completed</a></li>
      <li><a href="#tabs-4">Requests</a></li>
    </ul>
    <div id="tabs-1">
     <%= render "unassigned" %>
    </div>
    <div id="tabs-2">
     Current Projects
    </div>
    <div id="tabs-3">
      Completed
    </div>
    <div id="tabs-4">
     <%= render "requests" %>
    </div>
  </div>
</div>
